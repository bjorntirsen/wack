<section class="second-section">
  <div class="container main__container row">
    <div class="col__sidebar">
      <h3 class="">Channels</h3>
      <ul id="channels">
        <% channels.forEach(channel => { %>
        <li class="channel__li">
          <a href="home/channels/<%= channel.id %>">#<%= channel.name %></a>
        </li>
        <% }) %>
        <li class="channel__li">
          <a href="home/channels/create">Create channel</a>
        </li>
      </ul>
    </div>
    <div class="col__users">
      <h3 class="col__header">Users Online</h3>
      <ul id="users"></ul>
    </div>
  </div>
  <div class="col__users">
    <h3 class="col__header">Users Offline</h3>
    <ul id="offlineUsers">
      <% if (users.length > 0) { %>
        <% users.forEach(item => { %>
          <% if (item._id !== user._id) { %>
            <a href="channels/startPM/<%= user.userId %>" class="user__li"><%= item.name %></>
          <% } %>
        <% }) %>
      <% } %>
    </ul>
  </div>
</section>
<!-- ----------JAVASCRIPT---------- -->
<script src="/socket.io/socket.io.js"></script>
<script src="public/scripts/script.js"></script>
